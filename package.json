{
  "name": "gatsby-starter-ts",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Yasin Khan",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "server": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "jest",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "@types/cors": "2.8.12",
    "add": "2.0.6",
    "axios": "0.27.1",
    "canvas-confetti": "1.5.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.0",
    "gatsby": "4.13.0",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-image": "2.12.1",
    "gatsby-plugin-manifest": "4.13.0",
    "gatsby-plugin-mdx": "3.13.0",
    "gatsby-plugin-nodejs": "0.7.0",
    "gatsby-plugin-offline": "5.13.0",
    "gatsby-plugin-react-helmet": "5.13.0",
    "gatsby-plugin-sharp": "4.12.1",
    "gatsby-source-filesystem": "4.12.1",
    "gatsby-transformer-sharp": "4.12.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-helmet": "6.1.0",
    "tailwind-scrollbar-hide": "1.1.7",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "yarn": "1.22.18"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.29",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.0",
    "@types/react-helmet": "6.1.5",
    "@types/webpack-env": "1.16.4",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "autoprefixer": "10.4.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-preset-gatsby": "2.9.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-standard": "5.0.0",
    "gatsby-plugin-postcss": "5.10.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "nodemon": "2.0.15",
    "postcss": "8.4.12",
    "prettier": "2.5.1",
    "standardx": "3.0.0",
    "tailwindcss": "3.0.23",
    "ts-jest": "27.1.3",
    "typescript": "4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
